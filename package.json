{
  "name": "static-forms-service",
  "version": "1.0.0",
  "description": "Service in charge of handling forms sent from static sites.",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "node esbuild.config.js",
    "start": "node dist/index.js",
    "migrate:make": "knex migrate:make -x ts --migrations-directory src/database/migrations",
    "migrate:latest": "tsx --env-file=.env node_modules/.bin/knex migrate:latest --knexfile ./src/knexfile.ts",
    "seed:make": "knex seed:make -x ts",
    "seed": "tsx --env-file=.env node_modules/.bin/knex seed:run --knexfile ./src/knexfile.ts"
  },
  "author": "Fabricio Borgobello",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/pg": "^8.10.9",
    "esbuild": "^0.19.8",
    "esbuild-plugin-alias": "^0.2.1",
    "tsx": "^4.6.0"
  }
}
